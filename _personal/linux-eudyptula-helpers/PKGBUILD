pkgname=linux-eudyptula-helpers
pkgver=1
pkgrel=1
pkgdesc="Helper files for Linux Eudyptula Kernel"
arch=('i686' 'x86_64')
url="http://thiagowfx.github.io/"
license=('GPL')
depends=('coreutils' 'linux-firmware' 'kmod' 'mkinitcpio>=0.7')
source=('linux-eudyptula.preset')
md5sums=('9ed2f171d4a0c5dd04b4cc990bd8ba71')

package() {
	cd "$srcdir"
    install -Dm644 linux-eudyptula.preset "$pkgdir/etc/mkinitcpio.d/linux-eudyptula.preset"
}
